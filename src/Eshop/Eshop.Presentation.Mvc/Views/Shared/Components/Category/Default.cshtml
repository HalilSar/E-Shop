@model  List<GetCategoryResponse>


<div class="list-group">
    @foreach (var item in Model)
    {
            <a class="list-group-item list-group-item-action list-group-item-light" asp-action="GetByCategoryId" asp-controller="Home"  asp-route-CategoryId="@item.Id">
                <h6>@item.Title</h6> 
            
            </a>  
    }
</div>

